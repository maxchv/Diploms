@model IEnumerable<string>


@foreach (var link in Model)
{
    <div style="margin-bottom:10%;padding-left: 5px; padding-right: 5px;">
        @Html.RouteLink(link, new
   {
       controller = "RealEstate",
       action = "ListAll",
       typeOperation = link,
       page = 1
   }, new
   {
       @class = "buttonBtn col-xs-12"
            + (link == ViewBag.SelectedTypeOperation ? " buttonPrm" : "")
   })
    </div>
}
<div style="padding-left: 5px; padding-right: 5px;">
    @Html.ActionLink("Вся недвижимость", "ListAll", "RealEstate", null,
    new { @class = "buttonBtn col-xs-12" })
</div>
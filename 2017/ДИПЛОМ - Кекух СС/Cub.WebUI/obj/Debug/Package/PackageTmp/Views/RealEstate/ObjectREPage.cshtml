@model Cub.Domian.Entitis.RealEstate
@{
    ViewBag.Title = "Объект недвижимости";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/Site.css" rel="stylesheet" />
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="row panel">

                <div id="categories" class="col-md-3" style="padding-right: 5px;">
                    <div class="col-md-12">
                        @Html.Action("MenuAll", "Nav")
                    </div>
                    
                </div>
                <div class="col-md-9" style="margin-left:0; padding-left: 0px;">
                    
                        @Html.Partial("_ObjectRESummary", Model)
                        <div class="col-xs-5 pull-left" style="padding-left:0;">
                            @Html.ActionLink("Вернуться назад", "ListAll", "RealEstate", new { item_id = Model.Id }, new { @class = "viewDialog buttonBtn btn-block  btn-lg", data_dialog_title = "Заполнение заявки" })

                        </div>
                        <div class="col-xs-5 pull-right" style="padding-right:0;">
                            @Html.ActionLink("Заполнить заявку", "ModalForm", "Order", new { item_id = Model.Id }, new { @class = "compItem buttonBtn btn-block  btn-lg", data_dialog_title = "Заполнение заявки" })

                        </div>
                    </div>
                </div>
            </div>
    </div>
</div>

